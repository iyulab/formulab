{
  "name": "formulab",
  "version": "0.1.0",
  "description": "Manufacturing & Engineering calculation formulas library - 100+ industrial calculations for OEE, Cpk, metal weight, logistics, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./quality": {
      "types": "./dist/quality/index.d.ts",
      "import": "./dist/quality/index.js"
    },
    "./metal": {
      "types": "./dist/metal/index.d.ts",
      "import": "./dist/metal/index.js"
    },
    "./chemical": {
      "types": "./dist/chemical/index.d.ts",
      "import": "./dist/chemical/index.js"
    },
    "./electronics": {
      "types": "./dist/electronics/index.d.ts",
      "import": "./dist/electronics/index.js"
    },
    "./construction": {
      "types": "./dist/construction/index.d.ts",
      "import": "./dist/construction/index.js"
    },
    "./automotive": {
      "types": "./dist/automotive/index.d.ts",
      "import": "./dist/automotive/index.js"
    },
    "./logistics": {
      "types": "./dist/logistics/index.d.ts",
      "import": "./dist/logistics/index.js"
    },
    "./energy": {
      "types": "./dist/energy/index.d.ts",
      "import": "./dist/energy/index.js"
    },
    "./safety": {
      "types": "./dist/safety/index.d.ts",
      "import": "./dist/safety/index.js"
    },
    "./food": {
      "types": "./dist/food/index.d.ts",
      "import": "./dist/food/index.js"
    },
    "./utility": {
      "types": "./dist/utility/index.d.ts",
      "import": "./dist/utility/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "manufacturing",
    "engineering",
    "industrial",
    "calculator",
    "formulas",
    "oee",
    "cpk",
    "quality",
    "six-sigma",
    "lean",
    "metal",
    "machining",
    "logistics",
    "supply-chain",
    "cbm",
    "eoq",
    "safety-stock",
    "construction",
    "automotive",
    "electronics",
    "smt",
    "pcb",
    "energy",
    "safety",
    "niosh",
    "haccp"
  ],
  "author": "iyulab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyulab/formulab.git"
  },
  "homepage": "https://github.com/iyulab/formulab#readme",
  "bugs": {
    "url": "https://github.com/iyulab/formulab/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
